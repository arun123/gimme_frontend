{
  "name": "library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run library:server:production --bundleDependencies all",
    "build:prod": "ng build --prod",
    "build:staging": "node --max_old_space_size=2048 \"$(which ng)\" build --configuration=staging --output-path=dist/logitbox-staging",
    "swap": "mv dist/library dist/library-bak && mv dist/library-staging dist/library && mv dist/library-bak dist/library-staging"
  },
  "private": true,
  "dependencies": {
